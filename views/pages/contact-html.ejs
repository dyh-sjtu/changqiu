<main>
    <!-- Page Banner -->
    <div class="contactUs-page-banner page-banner container-fluid">
        <!-- Container -->
        <div class="container">
            <div class="banner-content">
                <h3>联系我们</h3>
                <ol class="breadcrumb">
                    <li><a href="/index" title="首页">首页</a></li>
                    <li class="active">联系我们</li>
                </ol>
            </div>
        </div><!-- Container /- -->
    </div><!-- Page Banner /- -->

    <!-- Map Section -->
    <div class="map container-fluid no-left-padding no-right-padding" id="container" style="width:100%; height:350px">

    </div>
    <!--  Map Section /- -->

    <!-- Contact Us -->
    <div class="contact-us container-fluid no-left-padding no-right-padding">
        <!-- Container -->
        <div class="container">
            <div class="contact-heading">
                <h5>联系我们</h5>
            </div>
            <!-- Row -->
            <div class="row">
                <div class="col-md-5 col-sm-5 col-xs-12 mt-15">
                    <table class="table table-bordered">
                        <tbody>
                        <tr>
                            <th><i class="fa fa-long-arrow-right"></i></th>
                            <td>Shanghai Changqiu Intelligent Technology Inc.</td>
                        </tr>
                        <tr>
                            <th><i class="fa fa-map-marker"></i></th>
                            <td><%= company.address %></td>
                        </tr>
                        <tr>
                            <th><i class="fa fa-phone"></i></th>
                            <td><a href="tel:<%= company.tel %>" title="<%= company.tel %>"><%= company.tel %></a></td>
                        </tr>
                        <tr>
                            <th><i class="fa fa-mobile"></i></th>
                            <td><a href="tel:<%= company.phoneNumber %>" title="<%= company.phoneNumber %>"><%= company.phoneNumber %></a></td>
                        </tr>
                        <tr>
                            <th><i class="fa fa-envelope"></i></th>
                            <td><a href="mailto:<%= company.email %>" title="<%= company.email %>"><%= company.email %></a></td>
                        </tr>
                        <tr>
                            <th><i class="fa fa-globe"></i></th>
                            <td>http://www.shanghaichangqiu.com</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="col-md-7 col-sm-7 col-xs-12">
                    <form>
                        <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                            <label class="form-control-label" for="input_name">姓名</label>
                            <input type="text" class="form-control" placeholder="请输入您的姓名" name="contact-name" id="input_name" />
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                            <label class="form-control-label" for="input_tel">手机号/座机</label>
                            <input type="text" class="form-control" placeholder="请输入您的联系方式" name="contact-tel" id="input_tel" />
                        </div>
                        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                            <label class="form-control-label" for="input_email">邮箱</label>
                            <input type="text" class="form-control" placeholder="请输入您的邮箱" name="contact-subject" id="input_email" />
                        </div>
                        <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                            <label class="form-control-label" for="textarea_message">留言</label>
                            <textarea class="form-control" placeholder="请输入您的留言反馈" rows="6" name="textarea-message" id="textarea_message"></textarea>
                        </div>
                        <div class="form-group col-md-12 col-sm-12 col-xs-12">
                            <button title="提交" type="submit" id="btn_submit" name="post">提交</button>
                        </div>
                        <div id="alert-msg" class="alert-msg"></div>
                    </form>
                </div>
            </div><!-- Row /- -->
        </div><!-- Container /- -->
    </div><!-- Contact Us - -->
</main>